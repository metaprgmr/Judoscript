<function sig="formatDate format, timezone" alias="fmtDate">
  Returns a string representing this date object in <param>format</param> and
  <param>timezone</param>, if specified.
</function>

<function sig="next period">
  Returns a new date object that is for the next <param>period</param>, which
  can be 'year', 'month', 'day', 'hour', 'minute' and 'second'.
</function>

<function sig="before datetime">
  Returns <!true> if this date object is before <param>datetime</param>.
  If no <param>datetime</param> specified, compare to the current date.
</function>

<function sig="after datetime">
  Returns <!true> if this date object is after <param>datetime</param>.
  If no <param>datetime</param> specified, compare to the current date.
</function>

<function sig="getTime">
  Returns the integer value of this date/time object.
</function>

<function sig="setTime datetime">
  Sets the time value to <param>datetime</param>.
  If no <param>datetime</param> specified, set to current time.
</function>
