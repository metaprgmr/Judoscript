connect localhost:7001 system weblogic
appname=timeoff
deployer=/DeployerRuntime/DeployerRuntime
invoke ${deployer}/remove $appname null null
task = $LAST
$task getState
state = $LAST
while $state <= 1
  echo "undeployment task running"
  sleep 1000
  $task getState
  state = $LAST
end
$task getStatus
status = $LAST
echo "undeployment task finished with status =" $status


