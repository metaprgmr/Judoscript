REM the mbean for the target server MUST be an admin MBEAN, not a config MBEAN

java weblogic.Admin -adminurl localhost:7001 -username system -password weblogic INVOKE -mbean "JavaOneDomain:Name=myPool,Type=JDBCConnectionPool" -method removeTarget "JavaOneDomain:Name=managed,Type=Server"
java weblogic.Admin -adminurl localhost:7001 -username system -password weblogic DELETE -mbean "JavaOneDomain:Name=myPool,Type=JDBCConnectionPool" 



